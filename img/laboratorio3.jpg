<svg xmlns="http://www.w3.org/2000/svg" width="800" height="500" viewBox="0 0 800 500">
  <rect width="800" height="500" fill="#f0f0f0"/>
  <rect x="50" y="50" width="700" height="400" rx="10" fill="#e0e0e0" stroke="#005b99" stroke-width="2"/>
  
  <!-- Sala de simulación -->
  <rect x="100" y="100" width="600" height="300" rx="5" fill="#ffffff" stroke="#005b99" stroke-width="2"/>
  
  <!-- Computadoras y estaciones de trabajo -->
  <rect x="130" y="130" width="120" height="80" rx="5" fill="#d1e6fa" stroke="#005b99" stroke-width="2"/>
  <rect x="270" y="130" width="120" height="80" rx="5" fill="#d1e6fa" stroke="#005b99" stroke-width="2"/>
  <rect x="410" y="130" width="120" height="80" rx="5" fill="#d1e6fa" stroke="#005b99" stroke-width="2"/>
  <rect x="550" y="130" width="120" height="80" rx="5" fill="#d1e6fa" stroke="#005b99" stroke-width="2"/>
  
  <!-- Monitores -->
  <rect x="150" y="150" width="80" height="40" rx="2" fill="#ffffff" stroke="#005b99" stroke-width="1"/>
  <rect x="290" y="150" width="80" height="40" rx="2" fill="#ffffff" stroke="#005b99" stroke-width="1"/>
  <rect x="430" y="150" width="80" height="40" rx="2" fill="#ffffff" stroke="#005b99" stroke-width="1"/>
  <rect x="570" y="150" width="80" height="40" rx="2" fill="#ffffff" stroke="#005b99" stroke-width="1"/>
  
  <!-- Área de prototipado -->
  <rect x="130" y="240" width="540" height="120" rx="5" fill="#d1e6fa" stroke="#005b99" stroke-width="2"/>
  
  <!-- Prototipos -->
  <circle cx="200" cy="300" r="30" fill="#ff6600" stroke="#005b99" stroke-width="1"/>
  <rect x="250" y="270" width="60" height="60" rx="5" fill="#ff6600" stroke="#005b99" stroke-width="1"/>
  <polygon points="350,270 380,300 350,330 320,300" fill="#ff6600" stroke="#005b99" stroke-width="1"/>
  <ellipse cx="450" cy="300" rx="40" ry="25" fill="#ff6600" stroke="#005b99" stroke-width="1"/>
  <rect x="500" y="280" width="100" height="40" rx="20" fill="#ff6600" stroke="#005b99" stroke-width="1"/>
  
  <!-- Texto -->
  <text x="400" y="450" font-family="Arial" font-size="24" fill="#005b99" text-anchor="middle">Sala de Simulación y Prototipado</text>
  <text x="400" y="80" font-family="Arial" font-size="18" fill="#ff6600" text-anchor="middle">SENA - Servicio Nacional de Aprendizaje</text>
</svg>